{
  "plugins": ["jasmine"],
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:jasmine/recommended"],
  "env": {
    "node": true,
    "jasmine": true
  },
  "rules": {
    "arrow-parens": 0,
    "global-require": 0,
    "import/first": 0,
    "import/prefer-default-export": 0,
    "indent": 0,
    "no-confusing-arrow": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "jasmine/no-spec-dupes": 0,
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
